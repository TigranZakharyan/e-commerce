{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-restricted-exports": [
      "error",
      {
        "restrictedNamedExports": [
          "default"
        ]
      }
    ],
    "array-bracket-spacing": [
      "error",
      "always",
      {
        "singleValue": false
      }
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-unused-expressions": "error"
  }
}
